        "cross-env": "^6.0.3",
        "cypress": "^4.0.1",
        "glob-all": "^3.1.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.15",
        "purify-css": "^1.2.5",
        "purifycss-webpack": "^0.7.0",
        "resolve-url-loader": "^3.1.0",
